{
  "name": "hobnob-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist/ && dotenv webpack",
    "serve": "dotenv ./scripts/serve.sh",
    "test:e2e": "yarn run test:e2e:all",
    "test:e2e:all": "yarn run test:e2e:chrome firefox",
    "test:e2e:chrome": "./scripts/e2e.test.sh chrome",
    "test:e2e:firefox": "./scripts/e2e.test.sh firefox",
    "api:init": "git submodule update --init",
    "api:install": "yarn --cwd api install",
    "api:serve": "yarn --cwd api run build && dotenv -e api/envs/.env.example node api/dist/index.js",
    "api:update": "git submodule update --init --remote"
  },
